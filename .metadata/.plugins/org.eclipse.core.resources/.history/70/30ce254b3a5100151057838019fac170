/**
 * Count number of binary strings without consecutive 1â€™s
 * @author sahil
 */
public class CountNumberOfBinaryStrings {

	public static void main(String[] args) {
		int n=5;
		int a[]=new int[n];
		int b[]=new int[n];
		
		a[1]=1;b[1]=1;
		int i=2;
		while(i<n){
			a[i]=a[i-1]+b[i-1];
			b[i]=a[i-1];
		}
	}

}
